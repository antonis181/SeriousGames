<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Suitcase_FrameLayoutEz"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SuitcaseEz"
    android:layout_gravity="center"
    android:padding="24dp"
    app:layout_constraintDimensionRatio="w,1:1"
    >

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        android:id="@+id/Suitcase_LinearLayout1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/Suitcase_LinearLayout2"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/suitcase_base">


        <GridLayout
            android:id="@+id/Suitcase_GridLayout1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:rowCount="2"
            android:columnCount="2"
            android:layout_marginLeft="40sp"
            android:layout_marginRight="20sp"
            android:layout_marginBottom="250sp"
            android:layout_marginTop="250sp"
            android:layout_gravity="center">

            <ImageView
                android:id="@+id/Suitcase_Base_imageView1"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                android:layout_gravity="fill_horizontal"
                android:contentDescription="@string/imageDesc"
                android:layout_margin="4dp" />

            <ImageView
                android:id="@+id/Suitcase_Base_imageView2"
                android:src="@drawable/suitcase_accesscard"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                android:layout_gravity="fill_horizontal"
                android:contentDescription="@string/imageDesc"
                android:layout_margin="4dp" />

            <ImageView
                android:id="@+id/Suitcase_Base_imageView3"
                android:src="@drawable/thing_1"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                android:layout_gravity="fill_horizontal"
                android:contentDescription="@string/imageDesc"
                android:layout_margin="4dp" />

            <ImageView
                android:id="@+id/Suitcase_Base_imageView4"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                android:layout_gravity="fill_horizontal"
                android:contentDescription="@string/imageDesc"
                android:layout_margin="4dp" />

        </GridLayout>

    </LinearLayout>


    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        android:id="@+id/Suitcase_LinearLayout2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/Suitcase_LinearLayout1"
        android:background="@drawable/suitcase_lid">


        <GridLayout
            android:id="@+id/Suitcase_GridLayout2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:rowCount="2"
            android:columnCount="2"
            android:layout_marginRight="40sp"
            android:layout_marginLeft="20sp"
            android:layout_marginBottom="250sp"
            android:layout_marginTop="250sp"
            android:layout_gravity="center">

            <ImageView
                android:id="@+id/Suitcase_Lid_imageView1"
                android:src="@drawable/thing_1"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                android:layout_gravity="fill_horizontal"
                android:contentDescription="@string/imageDesc"
                android:layout_margin="4dp" />

            <ImageView
                android:id="@+id/Suitcase_Lid_imageView2"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                android:layout_gravity="fill_horizontal"
                android:contentDescription="@string/imageDesc"
                android:layout_margin="4dp" />

            <ImageView
                android:id="@+id/Suitcase_Lid_imageView3"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                android:layout_gravity="fill_horizontal"
                android:contentDescription="@string/imageDesc"
                android:layout_margin="4dp" />

            <ImageView
                android:id="@+id/Suitcase_Lid_imageView4"
                android:src="@drawable/thing_1"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                android:layout_gravity="fill_horizontal"
                android:contentDescription="@string/imageDesc"
                android:layout_margin="4dp" />


        </GridLayout>


    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>